{
  "name": "atenxion-emr",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && cd client && npm ci && npm run build",
    "start": "node dist/index.js",
    "dev": "concurrently -k -n API,WEB \"ts-node-dev src/index.ts\" \"cd client && npm run dev\"",
    "migrate": "prisma migrate deploy",
    "seed:csv": "ts-node scripts/loadCsv.ts",
    "test": "jest --runInBand",
    "lint": "eslint .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "devDependencies": {
    "typescript": "^5",
    "ts-node-dev": "^2",
    "@types/node": "^20",
    "eslint": "^9",
    "@typescript-eslint/parser": "^7",
    "@typescript-eslint/eslint-plugin": "^7",
    "jest": "^29",
    "ts-jest": "^29",
    "@types/jest": "^29",
    "supertest": "^6",
    "@types/supertest": "^2",
    "concurrently": "^9"
  },
  "dependencies": {
    "express": "^4",
    "cors": "^2",
    "helmet": "^7",
    "morgan": "^1",
    "zod": "^3",
    "jsonwebtoken": "^9",
    "bcrypt": "^5",
    "express-rate-limit": "^7",
    "@prisma/client": "^5",
    "prisma": "^5"
  }
}
